{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd ../.. && pnpm build --filter=api",
  "framework": "nextjs",
  "installCommand": "cd ../.. && pnpm install",
  "ignoreCommand": "node scripts/skip-ci.js",
  "crons": [
    {
      "path": "/cron/keep-alive",
      "schedule": "0 1 * * *"
    }
  ]
}

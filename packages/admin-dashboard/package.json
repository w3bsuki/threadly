{
  "name": "@repo/admin-dashboard",
  "version": "0.1.0",
  "description": "Admin dashboard components for content moderation and platform management",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "build": "node scripts/conditional-build.js",
    "dev": "tsup --watch",
    "lint": "biome lint src/",
    "format": "biome format src/",
